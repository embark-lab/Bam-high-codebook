}

@article{thorntonAnorexiaNervosaGenetics2018,
  title = {The {{Anorexia Nervosa Genetics Initiative}} ({{ANGI}}): {{Overview}} and Methods},
  shorttitle = {The {{Anorexia Nervosa Genetics Initiative}} ({{ANGI}})},
