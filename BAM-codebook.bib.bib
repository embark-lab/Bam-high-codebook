@article{palermoExaminingImpactWeight2021,
  title = {Examining the Impact of Weight Bias on the Association between Exercise Identity and Maladaptive Exercise Behaviors},
  author = {Palermo, Madeline and Staples, Cody and Rancourt, Diana},
